{
  "expo": {
    "name": "alloha",
    "scheme": "alloha",
    "slug": "alloha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-font",
        {
          "SUIT-Bold": ["./assets/fonts/SUIT-Bold.otf"],
          "SUIT-ExtraBold": ["./assets/fonts/SUIT-ExtraBold.otf"],
          "SUIT-ExtraLight": ["./assets/fonts/SUIT-ExtraLight.otf"],
          "SUIT-Heavy": ["./assets/fonts/SUIT-Heavy.otf"],
          "SUIT-Light": ["./assets/fonts/SUIT-Light.otf"],
          "SUIT-Medium": ["./assets/fonts/SUIT-Medium.otf"],
          "SUIT-Regular": ["./assets/fonts/SUIT-Regular.otf"],
          "SUIT-SemiBold": ["./assets/fonts/SUIT-SemiBold.otf"],
          "SUIT-Thin": ["./assets/fonts/SUIT-Thin.otf"],
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]

    ],
    "ios": {
      "buildNumber": "3",
      "bundleIdentifier": "com.akmz.alloha",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "출퇴근 체크를 위한 위치권한이 필요합니다.",
        "NSLocationAlwaysUsageDescription": "App requires geolocation to improve the quality of the service",
        "NSLocationWhenInUseUsageDescription": "출퇴근 체크를 위한 위치권한이 필요합니다."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "FOREGROUND_SERVICE",
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.akmz.alloha"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0dd79bb6-37ee-41de-ab18-ae35b01e55a2"
      }
    },
    "owner": "akmz",
  },
  "name": "alloha"
}