{
  "expo": {
    "name": "알로하",
    "scheme": "aloha",
    "slug": "alloha",
    "version": "1.0.11",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-font",
        {
          "SUIT-Bold": [
            "./assets/fonts/SUIT-Bold.otf"
          ],
          "SUIT-ExtraBold": [
            "./assets/fonts/SUIT-ExtraBold.otf"
          ],
          "SUIT-ExtraLight": [
            "./assets/fonts/SUIT-ExtraLight.otf"
          ],
          "SUIT-Heavy": [
            "./assets/fonts/SUIT-Heavy.otf"
          ],
          "SUIT-Light": [
            "./assets/fonts/SUIT-Light.otf"
          ],
          "SUIT-Medium": [
            "./assets/fonts/SUIT-Medium.otf"
          ],
          "SUIT-Regular": [
            "./assets/fonts/SUIT-Regular.otf"
          ],
          "SUIT-SemiBold": [
            "./assets/fonts/SUIT-SemiBold.otf"
          ],
          "SUIT-Thin": [
            "./assets/fonts/SUIT-Thin.otf"
          ],
          "Tium": [
            "./assets/fonts/Tium.ttf"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isIosBackgroundLocationEnabled": false,
          "isAndroidBackgroundLocationEnabled": false
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-updates",
        {
          "username": "akmz"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "buildNumber": "28",
      "bundleIdentifier": "com.akmz.alloha",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "출퇴근 체크를 위한 위치권한이 필요합니다.",
        "NSLocationAlwaysUsageDescription": "출퇴근 체크를 위한 위치권한이 필요합니다.",
        "NSLocationWhenInUseUsageDescription": "출퇴근 체크를 위한 위치권한이 필요합니다."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "FOREGROUND_SERVICE",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "googleServicesFile": "./google-services.json",
      "package": "com.akmz.alloha",
      "versionCode": 27
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0dd79bb6-37ee-41de-ab18-ae35b01e55a2"
      }
    },
    "owner": "akmz"
  },
  "name": "aloha"
}
